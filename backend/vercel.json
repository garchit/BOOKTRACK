


{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "SQLALCHEMY_DATABASE_URI": "sqlite:///users.db",  // Change to your production DB URI
    "JWT_SECRET_KEY": "grocery",                     // Replace with your actual secret key
    
  }
}
